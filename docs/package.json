{
  "name": "@go-ui/docs",
  "version": "0.0.1",
  "description": "Go UI documentation site",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "scripts": {
    "prerender": "stencil build --prerender",
    "build.ia": "ts-node --esm ./bin/build-ia.ts",
    "watch.ia": "ts-node --esm ./bin/build-ia.ts --watch",
    "build": "pnpm run build.ia && pnpm run prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@go-ui/core": "workspace:0.0.0-development",
    "@stencil/core": "^2.22.1",
    "@stencil/helmet": "^0.3.3",
    "@stencil/sass": "^1.5.2",
    "@stencil/store": "^2.0.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^18.11.18",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^6.5.1",
    "cpr": "^3.0.1",
    "directory-tree": "^3.5.1",
    "esm": "^3.2.25",
    "highlight.js": "^11.7.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "minimist": "^1.2.7",
    "nanospinner": "^1.1.0",
    "puppeteer": "^10.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "workbox-build": "^6.5.4"
  },
  "license": "MIT",
  "dependencies": {
    "copy-text-to-clipboard": "^3.0.1",
    "json5": "^2.2.3",
    "lodash-es": "^4.17.21",
    "lodash.kebabcase": "^4.1.1",
    "lodash.startcase": "^4.4.0",
    "lodash.uniqueid": "^4.0.1",
    "markdown-it": "^13.0.1",
    "markdown-it-include": "^2.0.0",
    "markdown-it-meta": "^0.0.1",
    "markdown-it-title": "^4.0.0",
    "pretty": "^2.0.0",
    "stencil-router-v2": "^0.6.0"
  }
}
