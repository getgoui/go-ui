{
  "name": "@go-ui/docs",
  "version": "0.0.1",
  "description": "Go UI documentation site",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "",
    "build": "cpr content www/content -d && stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@go-ui/core": "workspace:*",
    "@go-ui/docsite-utils": "workspace:*",
    "@stencil/core": "2.17.3",
    "@stencil/helmet": "^0.3.3",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.5.2",
    "autoprefixer": "^10.4.8",
    "cpr": "^3.0.1",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "postcss": "^8.4.16",
    "puppeteer": "^10.4.0",
    "tailwindcss": "^3.1.8",
    "workbox-build": "^6.5.4"
  },
  "license": "MIT",
  "dependencies": {
    "highlight.js": "^11.6.0",
    "markdown-it": "^13.0.1",
    "markdown-it-include": "^2.0.0",
    "markdown-it-meta": "^0.0.1",
    "pretty": "^2.0.0",
    "stencil-router-v2": "^0.6.0"
  }
}
