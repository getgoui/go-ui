{
  "scripts": {
    "dev": "concurrently --kill-others \"npm:watch.*\"",
    "watch.core": "nx build @go-ui/core -- --watch",
    "watch.ia": "nx watch.ia @go-ui/docs",
    "watch.docs": "nx start @go-ui/docs",
    "dev.core": "nx start @go-ui/core",
    "dev.docs": "nx build @go-ui/core && nx start @go-ui/docs",
    "dev.react.app": "nx start @go-ui/react-starter",
    "build": "nx run-many --target=build",
    "build.docs": "nx build @go-ui/docs",
    "build.ia": "nx build.ia @go-ui/docs",
    "build.core": "nx build @go-ui/core",
    "build.react": "nx build @go-ui/react",
    "build.vue": "nx build @go-ui/vue",
    "build.affected": "nx affected:build",
    "test.unit.core": "nx test.spec @go-ui/core",
    "test.e2e.core": "nx test.e2e @go-ui/core",
    "release.core": "nx semantic-release @go-ui/core",
    "release.react": "nx semantic-release @go-ui/react",
    "release.vue": "nx semantic-release @go-ui/vue",
    "gg": "node ./bin/index.js"
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^6.5.1",
    "cpr": "^3.0.1",
    "esm": "^3.2.25",
    "lodash.camelcase": "^4.3.0",
    "lodash.debounce": "^4.0.8",
    "lodash.kebabcase": "^4.1.1",
    "minimist": "^1.2.6",
    "nanospinner": "^1.1.0",
    "nx": "14.6.5",
    "prompts": "^2.4.2",
    "semantic-release": "^19.0.5",
    "semantic-release-cli": "^5.4.4",
    "semantic-release-monorepo": "^7.0.5"
  }
}
