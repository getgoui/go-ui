{
  "name": "@go-ui/core",
  "version": "0.0.0-development",
  "description": "Go Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/go-components/go-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "src/global/"
  ],
  "bin": {
    "gg": "./bin/index.js"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "concurrently --kill-others \"npm run watch.components\" \"npm run docs.start\"",
    "serve.demo": "stencil build --dev --watch --serve",
    "watch.components": "stencil build --dev --docs-readme --watch",
    "docs.start": "cd docs && npm start",
    "docs.build": "npm run build && cd docs && npm i && npm run build",
    "test.spec": "stencil test --spec",
    "test.spec.watch": "stencil test --spec --watchAll",
    "test.e2e": "stencil test --e2e",
    "test.e2e.watch": "stencil test --e2e --watchAll",
    "test.e2e.screenshot": "stencil test --e2e --screenshot",
    "generate": "gg generate",
    "stylelint": "stylelint \"./src/**/*.scss\"",
    "release": "standard-version",
    "husky.install.hooks": "husky install",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@stencil/core": "^2.11.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.3.2",
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^26.0.24",
    "@types/puppeteer": "^5.4.4",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.3.5",
    "chalk": "^4.1.2",
    "concurrently": "^6.4.0",
    "cssnano": "^5.0.12",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "minimist": "^1.2.5",
    "postcss-pxtorem": "^6.0.0",
    "prompts": "^2.4.2",
    "puppeteer": "^5.5.0",
    "semantic-release-cli": "^5.4.4",
    "standard-version": "^9.3.2",
    "stylelint": "^14.1.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard-scss": "^2.0.1",
    "yargs": "^17.3.0",
    "semantic-release": "^18.0.1"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/go-components/go-components.git"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
