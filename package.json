{
  "name": "@go-ui/core",
  "version": "0.0.9",
  "description": "Go Components",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/go-components/go-components.esm.js",
  "files": [
    "dist/",
    "loader/",
    "src/global/"
  ],
  "bin": {
    "gg": "./bin/index.js"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run docs.start\"",
    "serve": "stencil build --dev --watch --serve",
    "dev": "stencil build --dev --docs-readme --watch",
    "docs.start": "cd docs && npm run clear && npm start",
    "docs.build": "npm run build && cd docs && npm i && npm run build",
    "test.spec": "stencil test --spec",
    "test.spec.watch": "stencil test --spec --watchAll",
    "test.e2e": "stencil test --e2e",
    "test.e2e.watch": "stencil test --e2e --watchAll",
    "test.e2e.screenshot": "stencil test --e2e --screenshot",
    "generate": "gg generate",
    "stylelint": "stylelint \"./src/**/*.scss\"",
    "release": "standard-version",
    "husky.install.hooks": "husky install"
  },
  "dependencies": {
    "@stencil/core": "^2.9.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "@axe-core/puppeteer": "^4.2.2",
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/sass": "^1.4.1",
    "@types/jest": "^26.0.21",
    "@types/puppeteer": "^5.4.3",
    "autoprefixer": "^10.3.1",
    "axe-core": "^4.3.3",
    "chalk": "^4.1.2",
    "concurrently": "^6.2.1",
    "cssnano": "^5.0.8",
    "esm": "^3.2.25",
    "husky": "^7.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "minimist": "^1.2.5",
    "postcss-pxtorem": "^6.0.0",
    "prompts": "^2.4.1",
    "puppeteer": "^5.5.0",
    "standard-version": "^9.3.1",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-prettier": "^8.0.2",
    "yargs": "^17.1.1"
  },
  "license": "MIT"
}
