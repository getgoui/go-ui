{
  "name": "@go-ui/docsite-utils",
  "version": "1.0.0",
  "description": "Utility components for Go UI document site",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/go-utils/go-utils.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/getgoui/go-utils"
  },
  "files": [
    "dist/"
  ],
  "homepage": "https://go-ui.com/",
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "predeploy": "pnpm run build && git add . && git commit -m \"Pre deploy cleanup\"",
    "deploy": "npm version patch && npm publish --access public"
  },
  "dependencies": {
    "@go-ui/core": "workspace:0.0.0-development",
    "@stencil/core": "^2.17.4",
    "copy-text-to-clipboard": "^3.0.1",
    "json5": "^2.2.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "postcss": "^8.4.16",
    "prismjs": "^1.29.0"
  },
  "devDependencies": {
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^1.5.2",
    "@types/jest": "^27.5.2",
    "autoprefixer": "^10.4.8",
    "cssnano": "^5.1.13",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "postcss-pxtorem": "^6.0.0",
    "puppeteer": "^10.4.0"
  },
  "license": "MIT"
}
