{
  "publishConfig": {
    "access": "public"
  },
  "github": {
    "release": true
  },
  "git": {
    "commitMessage": "chore: release v${version} [skip ci]",
    "requireCleanWorkingDir": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "conventionalcommits",
      "infile": "../../CHANGELOG.md"
    }
  }
}